<LanguageLib> <!-- Fredrik Andersson -->
  <Language lang="English">
    <Lang>English</Lang>
    <DayStart>Happy to see you again, @user</DayStart>
    <LunchStart>Have a pleasant meal.</LunchStart>
    <LunchEnd>Welcome back @user</LunchEnd>
    <DayEnd>Have a nice evening.</DayEnd>
    <WeekEnd>Have a nice weekend, take good care.</WeekEnd>
    <ErrorFlex>I'm sorry, @user. I'm afraid I can't do that.</ErrorFlex>
    <BufferTime>I'm sorry, @user. I'm afraid I can't do that.</BufferTime>
    <DoubleLogin>Just what do you think you're doing, @user?</DoubleLogin>
    <DoubleLunch>You are no hobbit, only one lunch @user</DoubleLunch>
    <TooSoon>I'm sorry, @user. I'm afraid I can't do that.</TooSoon>
    <NoEntryThisHour>@user, this conversation can serve no purpose anymore. Goodbye.</NoEntryThisHour>
    <UnExpectedError9000>This mission is too important for me to allow you to jeopardize it.</UnExpectedError9000>

    <LunchComment>Went to lunch</LunchComment>
    <SignOutComment>Signed out</SignOutComment>
  </Language>

  <Language lang="Finish"> <!--Paul Mikael Siippainen  -->
    <Lang>Finish</Lang>
    <DayStart>Hyvää huomenta @user</DayStart>
    <LunchStart>Hyvää ruokahalua @user</LunchStart>
    <LunchEnd>Tervetuloa takaisin lounaalta</LunchEnd>
    <DayEnd>Päivän työt tehty</DayEnd>
    <WeekEnd>Hyvää viikonloppua</WeekEnd>
    <ErrorFlex>Kirjautuminen epäonnistui. Tarkista Flex.</ErrorFlex>
    <BufferTime>Sisäänkirjautuminen ei onnistu lähellä lounasaikaa</BufferTime>
    <DoubleLogin>Kaksoiskirjautuminen ei onnistu</DoubleLogin>
    <DoubleLunch>You are no hobbit, only one lunch @user</DoubleLunch>
    <TooSoon>Ole hyvä ja odota lounasaikaan</TooSoon>
    <NoEntryThisHour>Tähän aikaan kirjauduttava webbi-sivun kautta</NoEntryThisHour>
    <UnExpectedError9000>Odottamaton häiriö</UnExpectedError9000>    
    
    <LunchComment>Lounas</LunchComment>
    <SignOutComment>Lähtenyt tältäpäivältä</SignOutComment>
  </Language>


  <Language lang="Norwegian">
    <Lang>Norwegian</Lang>
<!--    <DayStart>@user@olufkom</DayStart>
    <LunchStart>Kos deg med maten @user</LunchStart>
    <LunchEnd>Du overlevde denne gang og?</LunchEnd>
    -->
    <DayStart>@user@wav(Oluf.wav)</DayStart>
    <LunchStart>@wav(Mario Jump.wav)Da er det Lunsjtid @user</LunchStart>
    <LunchEnd>@wav(Mario Extra Life.wav)Lunsj ferdig nå @user</LunchEnd>
    <DayEnd>Jaha, du nsker ådra hjem @user</DayEnd>
    <WeekEnd>Hello @wav(Out of Bubble Gum.wav) . Ohh skith @wav(Mario Jump.wav) @wav(Mario Jump.wav) @wav(Mario Jump.wav) @wav(Pacman.wav)</WeekEnd>
    <ErrorFlex>Innloggingen ble ikke vellykket. Vennligst sjekk din Web Flex konto</ErrorFlex>
    <BufferTime>For tidlig for og dra på lunsj</BufferTime>
    <DoubleLogin>Du kan ikke logge inn to ganger</DoubleLogin>
    <DoubleLunch>You are no hobbit, only one lunch @user</DoubleLunch>
    <TooSoon>Rolig nå</TooSoon>
    <NoEntryThisHour>Du kan dessverre ikke være her mer i dag</NoEntryThisHour>
    <UnExpectedError9000>@wav(clown horn.wav)Den så vi ikke komme @wav(Oluf.wav)</UnExpectedError9000>
    
    <LunchComment>Lunsj</LunchComment>
    <SignOutComment>Gått for dagen</SignOutComment>
  </Language>


  <Language lang="Russian"> <!-- Konstantin (4475403239) edit, soon to be only audio, because yay-->
